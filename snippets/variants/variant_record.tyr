enum ShapeKind {
    Point,
    Circle,
    Square,
    Rectangle,
}

record Color {
    r: u8,
    g: u8,
    b: u8,
    a: u8,
}

record Shape {
    color: Color,
    case kind: ShapeKind {
        Point,
        Circle { radius: f32 },
        Square { length: f32 },
        Rectangle { width: f32, height: f32 },
    }
}

function main() {
    var red = Color { 255, 0, 0, 255 };
    var green = Color { 0, 255, 0, 255 };
}