enum Color {
    Red,
    Green,
    Blue,
}

enum TrafficLight {
    Red,
    Yellow,
    Green,
}

function print_color(c: Color) {
    switch c {
        case Red: println("Red"),
        case Green: println("Green"),
        case Blue: println("Blue"),
    }
}

function main() {
    var c = Color::Red;

    print_color(c);
    println(Color::Blue);

    c = Color::Green;
    c.print_color();
}