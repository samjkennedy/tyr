record Complex {
    real: f32,
    imag: f32
}

function add(a: Complex, b: Complex): Complex {
    return Complex {
        a.real + b.real,
        a.imag + b.imag
    };
}

function mul(a: Complex, b: Complex): Complex {
    return Complex {
        a.real * b.real - a.imag * b.imag,
        a.real * b.imag + a.imag * b.real
    };
}

