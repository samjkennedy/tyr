function main() {
    var a = [3]i32 {1, 2, 3};

    println(a[0]);
    println(a[1]);
    println(a[2]);

    a[1] = 4;

    println(a[0]);
    println(a[1]);
    println(a[2]);

    var x = 0;
    while x < 3 {
        println(a[x]);
        x = x + 1;
    }

    for x in 0..a.count {
        println(a[x]);
        x = x + 1;
    }
}