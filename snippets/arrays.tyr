function main() {
    var a = [1, 2, 3];

    print(a[0]);
    print(a[1]);
    print(a[2]);

    a[1] = 4;

    print(a[0]);
    print(a[1]);
    print(a[2]);

    var x = 0;
    while x < 3 {
        print(a[x]);
        x = x + 1;
    }
    x = 0;
    while x < 4 {
        print(a[x]);
        x = x + 1;
    }
}